{
  "name": "mutuelles_v3",
  "version": "0.1.0",
  "private": true,
  "description": "Application desktop Electron pour courtier â€“ base & design",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build && electron-builder -c electron-builder.yml",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "postinstall": "electron-builder install-app-deps",
    "db:reset": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs",
    "db:reset:seed": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs --seed",
    "db:reset:dry": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs --dry-run",
    "db:reset:keep-leads": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs --seed --preserve-tables \"clean_leads\"",
    "db:reset:keep-platforms": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs --seed --preserve-tables \"platforms_catalog,platform_credentials\"",
    "db:reset:keep-all-data": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs --preserve-tables \"clean_leads,platforms_catalog,platform_credentials,profiles\"",
    "db:migrate": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/migrate.mjs",
    "db:migrate:status": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/migrate.mjs --status",
    "db:migrate:dry": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/migrate.mjs --dry-run",
    "db:seed": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs",
    "db:seed:list": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --list",
    "db:seed:dry": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --dry-run",
    "db:seed:platforms": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only platforms",
    "db:seed:credentials": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only credentials",
    "db:seed:profiles": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only profiles",
    "db:status": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/status.mjs",
    "db:status:verbose": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/status.mjs --verbose",
    "db:dump": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/dump.mjs",
    "db:dump:schema": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/dump.mjs --schema-only",
    "db:dump:data": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/dump.mjs --data-only",
    "db:optimize": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/optimize.mjs",
    "db:generate-leads": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/generate-leads.mjs",
    "flow:run": "node cli/runner.mjs flow:run",
    "leads:list": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/leads/list.mjs",
    "leads:show": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/leads/show.mjs",
    "clean": "powershell -ExecutionPolicy Bypass -File ./clean-install.ps1"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "dotenv": "^16.4.5",
    "electron-log": "^5.4.3",
    "googleapis": "^164.1.0",
    "lucide-react": "^0.441.0",
    "playwright-core": "^1.48.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.1",
    "commander": "^11.0.0",
    "cross-env": "^10.1.0",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}
