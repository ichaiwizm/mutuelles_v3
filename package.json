{
  "name": "mutuelles_v3",
  "version": "0.1.0",
  "private": true,
  "description": "Application desktop Electron pour courtier â€“ base & design",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build && electron-builder -c electron-builder.yml",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "postinstall": "electron-builder install-app-deps",
    "flows:export": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/flows/export_flow.mjs",
    "flows:export:all": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/flows/export_flow.mjs --all",
    "flows:export:slug": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/flows/export_flow.mjs --slug",
    "flows:add": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/flows/add_flow.mjs",
    "flows:update": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/flows/update_flow.mjs",
    "flows:delete": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/flows/delete_flow.mjs",
    "flows:lint": "node scripts/flows/lint_flows.mjs",
    "flows:sync": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/flows/sync_flows.mjs",
    "flows:run": "cross-env ELECTRON_RUN_AS_NODE=1 electron admin/cli/run_file_flow.mjs",
    "flows:run:dev": "npm run flows:run -- --mode dev",
    "flows:run:devp": "npm run flows:run -- --mode dev_private",
    "db:reset": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs",
    "db:reset:seed": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs --seed",
    "db:reset:dry": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/reset.mjs --dry-run",
    "db:migrate": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/migrate.mjs",
    "db:migrate:status": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/migrate.mjs --status",
    "db:migrate:dry": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/migrate.mjs --dry-run",
    "db:seed": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs",
    "db:seed:list": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --list",
    "db:seed:dry": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --dry-run",
    "db:seed:flows": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only flows",
    "db:seed:leads": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only leads",
    "db:seed:platforms": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only platforms",
    "db:seed:credentials": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only credentials",
    "db:seed:profiles": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/seed.mjs --only profiles",
    "db:status": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/status.mjs",
    "db:status:verbose": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/db/status.mjs --verbose",
    "fields:import": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/metadata/import_fields.mjs"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "lucide-react": "^0.441.0",
    "playwright-core": "^1.48.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^2.3.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}
