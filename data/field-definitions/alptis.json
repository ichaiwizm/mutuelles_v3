{
  "auth.loginButton": {
    "selector": "button.login-btn, a.login-link, [href*='login']",
    "meta": {
      "label": "Login button"
    }
  },
  "auth.username": {
    "selector": "input[name='username'], input[type='email'], input[id*='username']",
    "meta": {
      "label": "Username field"
    }
  },
  "auth.password": {
    "selector": "input[name='password'], input[type='password'], input[id*='password']",
    "meta": {
      "label": "Password field"
    }
  },
  "auth.submit": {
    "selector": "button[type='submit'], button.submit, input[type='submit']",
    "meta": {
      "label": "Submit button"
    }
  },
  "consent.acceptAll": {
    "selector": "button:has-text('Tout autoriser'), button:has-text('Tout accepter'), [role='button']:has-text('Tout autoriser')",
    "meta": { "label": "Accept all cookies" }
  },
  "consent.manage": {
    "selector": "button:has-text('Préférence'), button:has-text('préférences'), button:has-text('Manage your preferences'), button:has-text('Manage your preferences about cookies')",
    "meta": { "label": "Open cookies preferences" }
  },
  "consent.overlay": {
    "selector": "#axeptio_overlay, .axeptio_mount",
    "meta": { "label": "Cookies overlay" }
  },
  "subscriber.firstName": {
    "selector": "input[name='firstName'], input[id*='firstName'], input[id*='prenom']",
    "meta": {
      "label": "First name"
    }
  },
  "subscriber.lastName": {
    "selector": "input[name='lastName'], input[id*='lastName'], input[id*='nom']",
    "meta": {
      "label": "Last name"
    }
  },
  "subscriber.email": {
    "selector": "input[name='email'], input[type='email'], input[id*='email']",
    "meta": {
      "label": "Email"
    }
  },
  "subscriber.birthDate": {
    "selector": "input[name='birthDate'], input[id*='birthDate'], input[id*='dateNaissance']",
    "adapter": "dateIsoToFr",
    "meta": {
      "label": "Birth date"
    }
  }
  ,
  "product.lastName": {
    "selector": "xpath=//label[contains(normalize-space(.),'Nom')]/following::input[1]",
    "meta": { "label": "Nom (produit)" }
  },
  "product.firstName": {
    "selector": "xpath=//label[contains(normalize-space(.),'Prénom')]/following::input[1]",
    "meta": { "label": "Prénom (produit)" }
  },
  "product.birthDate": {
    "selector": "xpath=//label[contains(normalize-space(.),'Date de naissance')]/following::input[1]",
    "adapter": "dateIsoToFr",
    "meta": { "label": "Date de naissance (produit)" }
  },
  "product.postalCode": {
    "selector": "xpath=//label[contains(normalize-space(.),'Code postal')]/following::input[1]",
    "meta": { "label": "Code postal (produit)" }
  }
}
