{
  "auth.loginButton": {
    "selector": "button.login-btn, a.login-link, [href*='login']",
    "meta": {
      "label": "Login button"
    }
  },
  "auth.username": {
    "selector": "input[name='username'], input[type='email'], input[id*='username']",
    "meta": {
      "label": "Username field"
    }
  },
  "auth.password": {
    "selector": "input[name='password'], input[type='password'], input[id*='password']",
    "meta": {
      "label": "Password field"
    }
  },
  "auth.submit": {
    "selector": "button[type='submit'], button.submit, input[type='submit']",
    "meta": {
      "label": "Submit button"
    }
  },
  "subscriber.firstName": {
    "selector": "input[name='firstName'], input[id*='firstName'], input[id*='prenom']",
    "meta": {
      "label": "First name"
    }
  },
  "subscriber.lastName": {
    "selector": "input[name='lastName'], input[id*='lastName'], input[id*='nom']",
    "meta": {
      "label": "Last name"
    }
  },
  "subscriber.email": {
    "selector": "input[name='email'], input[type='email'], input[id*='email']",
    "meta": {
      "label": "Email"
    }
  },
  "subscriber.birthDate": {
    "selector": "input[name='birthDate'], input[id*='birthDate'], input[id*='dateNaissance']",
    "adapter": "dateIsoToFr",
    "meta": {
      "label": "Birth date"
    }
  }
}
