{
  "platform": "swisslifeone",
  "fields": [
    {
      "key": "login_button_home",
      "type": "button",
      "label": "Bouton Se connecter (page d'accueil)",
      "selector": ".button-connection"
    },
    {
      "key": "login_username",
      "type": "text",
      "label": "Nom d'utilisateur",
      "selector": "#userNameInput"
    },
    {
      "key": "login_password",
      "type": "password",
      "label": "Mot de passe",
      "selector": "#passwordInput"
    },
    {
      "key": "login_submit",
      "type": "button",
      "label": "Connexion",
      "selector": "#submitButton"
    },
    {
      "key": "menu_etudes_simulateurs",
      "type": "button",
      "label": "Menu Études et simulateurs",
      "selector": "a:has-text('Études et simulateurs')"
    },
    {
      "key": "cookie_tout_autoriser",
      "type": "button",
      "label": "Tout autoriser (cookies)",
      "selector": "#onetrust-accept-btn-handler"
    },
    {
      "key": "menu_simulateurs",
      "type": "button",
      "label": "Menu Simulateurs",
      "selector": ".mat-mdc-menu-item.sous-lien:has-text('Simulateurs')"
    },
    {
      "key": "slsis_nom_projet",
      "type": "text",
      "label": "Nom du projet",
      "selector": "#nom-projet"
    },
    {
      "key": "slsis_date_effet",
      "type": "text",
      "label": "Date d'effet",
      "selector": "#contratSante-dateEffet"
    },
    {
      "key": "slsis_gamme",
      "type": "select",
      "label": "Gamme assurance",
      "selector": "#selection-produit-sante, [name='contratSante.produitId']"
    },
    {
      "key": "slsis_date_naissance",
      "type": "text",
      "label": "Date de naissance souscripteur",
      "selector": "#date-naissance-assure-principal"
    },
    {
      "key": "slsis_regime_social",
      "type": "select",
      "label": "Régime social",
      "selector": "#regime-social-assure-principal",
      "options": {
        "open_selector": "#regime-social-assure-principal",
        "option_selector_template": "#regime-social-assure-principal option[value='{{valueUpper}}']"
      }
    },
    {
      "key": "slsis_statut",
      "type": "select",
      "label": "Statut",
      "selector": "#statut-assure-principal",
      "options": {
        "open_selector": "#statut-assure-principal",
        "option_selector_template": "#statut-assure-principal option[value='{{valueUpper}}']"
      }
    },
    {
      "key": "slsis_departement",
      "type": "select",
      "label": "Département",
      "selector": "#departement-assure-principal",
      "options": {
        "open_selector": "#departement-assure-principal",
        "option_selector_template": "#departement-assure-principal option[value='{{value}}']"
      }
    },
    {
      "key": "slsis_couverture_oui",
      "type": "button",
      "label": "Couverture individuelle Oui (label)",
      "selector": "label[for='projet-sante-individuelle-oui']"
    },
    {
      "key": "slsis_couverture_non",
      "type": "button",
      "label": "Couverture individuelle Non (label)",
      "selector": "label[for='projet-sante-individuelle-non']"
    },
    {
      "key": "slsis_ij_oui",
      "type": "button",
      "label": "Indemnités journalières Oui (label)",
      "selector": "label[for='projet-confort-hospitalisation-oui']"
    },
    {
      "key": "slsis_ij_non",
      "type": "button",
      "label": "Indemnités journalières Non (label)",
      "selector": "label[for='projet-confort-hospitalisation-non']"
